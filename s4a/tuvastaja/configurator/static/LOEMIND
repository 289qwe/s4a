httpd.conf - httpd konfiguratsioon tuvastaja tarvis. Läheb failiks 
/var/www/conf/httpd.conf

aliases - maili aliaste fail, läheb failiks /etc/mail/aliases

nrpe.cfg - nrpe konfifail, läheb failiks /etc/nrpe.cfg

sendmail_* - sendmaili konfifaili staatilised osad, lähevad failiks /usr/share/sendmail/cf/$hostname.mc

syslog.conf - syslogi konfifail. läheb failiks /etc/syslog.conf

newsyslog.conf - newsyslogi konfifail. läheb failiks /etc/newsyslog.conf

sshd_config - ssh konfifail, läheb failiks /etc/ssh/sshd_config

variables.conf - snorti lokaalse konfi staatiline osa, läheb failiks /var/www/tuvastaja/data/snort/localconf/variables.conf

.profile - terminali konfifial, läheb failiks /root/.profile
